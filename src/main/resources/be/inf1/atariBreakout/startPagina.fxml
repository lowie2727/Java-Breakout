<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="be.inf1.atariBreakout.StartPaginaController">
    <Button fx:id="startButton" layoutX="495.0" layoutY="200.0" mnemonicParsing="false" prefWidth="110.0"
            text="Atari Breakout">
        <cursor>
            <Cursor fx:constant="HAND"/>
        </cursor>
    </Button>
    <Button fx:id="exitButton" layoutX="495.0" layoutY="345.0" mnemonicParsing="false" prefWidth="110.0" text="Exit">
        <cursor>
            <Cursor fx:constant="HAND"/>
        </cursor>
    </Button>
    <Text layoutX="200.0" layoutY="152.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Atari Breakout"
          textAlignment="CENTER">
        <font>
            <Font name="Algerian" size="83.0"/>
        </font>
    </Text>
    <Text layoutX="824.0" layoutY="581.0" strokeType="OUTSIDE" strokeWidth="0.0"
          text="Arnaud Paquet, Lowie Van Vyve, Jonas Vandenborne">
        <font>
            <Font name="Bell MT" size="12.0"/>
        </font>
    </Text>
    <Button fx:id="spelregelsButton" layoutX="495.0" layoutY="265.0" mnemonicParsing="false" prefWidth="110.0"
            text="Spelregels">
        <cursor>
            <Cursor fx:constant="HAND"/>
        </cursor>
    </Button>
    <Button fx:id="instellingenButton" layoutX="495.0" layoutY="305.0" mnemonicParsing="false" prefWidth="110.0"
            text="Instellingen">
        <cursor>
            <Cursor fx:constant="HAND"/>
        </cursor>
    </Button>
    <RadioButton fx:id="level1" layoutX="495.0" layoutY="235.0" mnemonicParsing="false">
        <toggleGroup>
            <ToggleGroup fx:id="levels"/>
        </toggleGroup>
        <cursor>
            <Cursor fx:constant="HAND"/>
        </cursor>
    </RadioButton>
    <RadioButton fx:id="level3" layoutX="589.0" layoutY="235.0" mnemonicParsing="false" toggleGroup="$levels">
        <cursor>
            <Cursor fx:constant="HAND"/>
        </cursor>
    </RadioButton>
    <RadioButton fx:id="level2" layoutX="542.0" layoutY="235.0" mnemonicParsing="false" toggleGroup="$levels">
        <cursor>
            <Cursor fx:constant="HAND"/>
        </cursor>
    </RadioButton>
</AnchorPane>
